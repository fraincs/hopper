{
    "$schema": "https://json.schemastore.org/eslintrc",
    "root": true,
    "extends": "plugin:@workleap/react-library",
    "overrides": [
        {
            "files": ["src/**/*.{js,jsx,ts,tsx}"],
            "rules": {
                "no-console": ["warn", { "allow": ["warn", "error"] }],
                "react/destructuring-assignment": "off",
                "no-param-reassign": "off"
            }
        }
    ]
}
